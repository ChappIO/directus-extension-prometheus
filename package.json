{
  "name": "directus-extension-prometheus",
  "description": "Export Directus metrics into prometheus",
  "icon": "extension",
  "version": "1.2.0",
  "license": "MIT",
  "homepage": "https://github.com/chappio/directus-extension-prometheus",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-hook",
    "prometheus",
    "metrics",
    "analytics"
  ],
  "author": {
    "name": "Thomas Biesart",
    "url": "https://github.com/chappio"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "dist/index.js",
    "source": "src/index.ts",
    "host": "^9.22.4"
  },
  "scripts": {
    "prepack": "directus-extension build && tsc",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "9.22.4",
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "prom-client": "^14.1.1"
  }
}
